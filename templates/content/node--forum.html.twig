{#
/**
 * @file
 * Theme override to display a forum node.
 *
 */
#}
<article{{ attributes }}>
    {{ title_prefix }}
        {% if label and not page %}
            <h2{{ title_attributes }}>
                <a href="{{ url }}" rel="bookmark">{{ label }}</a>
            </h2>
        {% endif %}
    {{ title_suffix }}
</article>
